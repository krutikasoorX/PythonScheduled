pipeline {
    agent any
    triggers {
        cron('*/5 * * * *')
    }
    environment {
        PYTHON_EXEC = 'C:\Users\Admin\AppData\Local\Programs\Python\Python311\python.exe'
        Num1 = '9'
        Num2 = '80'
    }
    stages {
        stage('version') {
            steps {
                 echo 'Running Python version...'
                 bat "${PYTHON_EXEC} --version"
            }
        }
        stage('hello') {
            steps {
                echo 'Running hello script...'
                bat "${PYTHON_EXEC} loopExec.py ${Num1} ${Num2}"
            }
        }
    }
}
