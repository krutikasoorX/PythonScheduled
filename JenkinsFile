pipeline {
    agent any
    triggers {
        cron('*/5 * * * *')
    }
    environment {
        Num1 = '9'
        Num2 = '80'
    }
    stages {
        stage('version') {
            steps {
                 echo 'Running Python version...'
                 bat 'python --version'
            }
        }
        stage('hello') {
            steps {
                echo 'Running hello script...'
                bat 'echo "Hello, Jenkins!"'
            }
        }
    }
}
