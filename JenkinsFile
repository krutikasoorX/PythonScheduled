pipeline {
    agent{
    docker{image : 'c73f7d8602800cf6fb88289039f86de31e6ab85d9bf5b358e032e889f2853a2e'}
    triggers {
        cron('*/5 * * * *')
    }
    environment {
        
        Num1 = '9'
        Num2 = '80'
    }
    stages {
        stage('version') {
            steps {
                 echo 'Running Python version...'
                 bat "python --version"
            }
        }
        stage('hello') {
            steps {
                echo 'Running hello script...'
                bat "python loopExec.py ${Num1} ${Num2}"
            }
        }
    }
}
