pipeline {
  agent any
  triggers {
        cron('*/5 * * * *')
    }
  environment {
        Num1 = 9
        Num2=80
    }
  stages {
    stage('version') {
      steps {
        bat 'py --version'
      }
    }
    stage('hello') {
      steps {
        bat 'py loopExec.py ${Num1} ${Num2}'
      }
    }
  }
}
