pipeline {
    agent{
    docker{image : 'node:20.15.0-alpine3.20'}
    triggers {
        cron('*/5 * * * *')
    }
    environment {
        
        Num1 = '9'
        Num2 = '80'
    }
    stages {
        stage('version') {
            steps {
                 echo 'Running Python version...'
                 bat "python --version"
            }
        }
        stage('hello') {
            steps {
                echo 'Running hello script...'
                bat "python loopExec.py ${Num1} ${Num2}"
            }
        }
    }
}
