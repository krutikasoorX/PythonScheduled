pipeline {
    agent {
        docker {
            image 'node:14'
        }
    }
    triggers {
        cron('*/5 * * * *')
    }
    environment {
        Num1 = '9'
        Num2 = '80'
    }
    stages {
        stage('Version') {
            steps {
                echo 'Running Python version...'
                bat "python --version"
            }
        }
        stage('Hello') {
            steps {
                echo 'Running hello script...'
                bat "python -x loopExec.py ${env.Num1} ${env.Num2}"
            }
        }
    }
}
